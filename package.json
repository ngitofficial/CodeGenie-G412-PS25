{
  "name": "codegenie",
  "displayName": "CodeGenie",
  "description": "VSCode extension for AI-powered code generation, debugging, and autocompletion",
  "version": "1.0.0",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:codegenie.run"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codegenie.run",
        "title": "CodeGenie"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "typescript": "^4.9.5",
    "@types/node": "^18.15.11"
  },
  "dependencies": {}
}
