{
  "name": "codegenie",
  "displayName": "CodeGenie",
  "description": "VSCode extension for AI-powered code generation, debugging, and autocompletion",
  "version": "1.0.0",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codegenie.run",
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codegenie.run",
        "title": "CodeGenie"
      }
    ]
  },
  
  "scripts": {
  "vscode:prepublish": "npm run compile",
  "compile": "tsc -p ./",
  "watch": "tsc -watch -p ./",
  "pretest": "npm run compile",
  "test": "node ./out/test/runTest.js"



  },
  "devDependencies": {
  "@types/vscode": "^1.85.0",      
  "@types/mocha": "^10.0.1",
  "@types/node": "^20.4.2",
  "typescript": "^5.0.0",
  "mocha": "^10.2.0",
  "@vscode/test-electron": "^2.3.0"
},

  "dependencies": {
    "axios": "^1.10.0"
  }
}
